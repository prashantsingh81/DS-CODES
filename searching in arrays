#include <stdio.h>
int main() {
int arr[5];
int n,count=0 , loc, upd;
printf(" enter the elements of array \n");
for(int i=0; i<5; i++){
scanf("%d",&arr[i]);
}
printf("enter the element you want to find\n");
scanf("%d",&n);
for(int i=0; i<5; i++){
if(arr[i] == n){
printf("%d found at location %d\n",n,i+1);
count +=1;
}
}
if(count == 0){
printf("%d not founded\n", n);
}
